 */
#define ____IAEOU__L1___         KC_Q,         KC_R,         KC_N,           KC_L,         KC_W
#define ____IAEOU__L2___ LCTL_T(KC_D), LALT_T(KC_S), LSFT_T(KC_T), LGUI_T(KC_SPC),         KC_G
#define ____IAEOU__L3___         KC_Z,         KC_X,         KC_C,           KC_V,         KC_J

#define      ____IAEOU__R1___             KC_Y,         KC_H,         KC_F,         KC_B,          KC_P
#define      ____IAEOU__R2___             KC_I, LGUI_T(KC_A), LSFT_T(KC_E),  LALT_T(KC_O), LCTL_T(KC_U) // [BUG] KC_SPC instead of KC_J
#define      ____IAEOU__R3___             KC_K,         KC_M,       KC_ENT,      KC_BSPC,        KC_ESC   // [bug]

//#elif defined(SIMPLE_30_LAYOUT)
#endif
//
// THUMB CLUSTER DEFINED ABOVE
//
// END OF [_IAEOU] 00
/////////////////////////////////////////////////////////////////////////////////////////////////// ###


#if defined(COMPREHENSIVE_30_LAYOUT)
/* [_QWERT] = LAYER 00 : QWERT DEFAULT LAYER
 * .--------------------------------------------.        .--------------------------------------------.
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |    Q   |    W   |    E   |    R   |    T   |        |    Y   |    U   |    I   |    O   |    P   |
 * |G[_1APP]|        |        |        |        |        |        |        |        |        |G[_1APP]|
 * | [_APPS]| [_POWR]| [_LEDS]| [_FUNC]| [_NUMB]|        | [_SYMB]| [_FUNC]| [_LEDS]| [_POWR]| [_APPS]|
 * |--------+--------+--------+--------+--------|        |--------+--------+--------+--------+--------|
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |    A   |    S   |    D   |    F   |    G   |        |    H   |    J   |    K   |    L   |  Space |
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |   LCtl |   LAlt |  LSft  |  LGui  | [_ACCN]|        | [_ACCN]|  LGui  |  LSft  |   LAlt |   LCtl |
 * |--------+--------+--------+--------+--------|        |--------+--------+--------+--------+--------|
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |    Z   |    X   |    C   |    V   |    B   |        |    N   |    M   |  Enter |Bckspace| Escape |
 * |        |        |        |        |        |        |        |        |        |        |        |
 * | [_DALY]| [_DVIM]| [_MOUS]| [_SYMB]| [_NUMB]|        | [_FVIM]| [_XVIM]| [_DVIM]|        | [_DALY]|
 * '--------------------------------------------'        '--------------------------------------------'
 */
#define ____QWERT__L1___ LT(_APPS, KC_Q),LT(_POWR, KC_W),LT(_LEDS, KC_E),LT(_FUNC, KC_R),LT(_NUMB,KC_T)
#define ____QWERT__L2___    LCTL_T(KC_A),   LALT_T(KC_S),   LSFT_T(KC_D),   LGUI_T(KC_F),LT(_ACCN,KC_G)
#define ____QWERT__L3___ LT(_DALY, KC_Z),LT(_DVIM, KC_X),LT(_MOUS, KC_C),LT(_SYMB, KC_V),LT(_NUMB,KC_B)

#define ____QWERT__R1___ LT(_SYMB,KC_Y),LT(_FUNC,KC_U), LT(_LEDS, KC_I),LT(_POWR,KC_O), LT(_APPS, KC_P)
#define ____QWERT__R2___ LT(_ACCN,KC_H),  LGUI_T(KC_J),    LSFT_T(KC_K),  LALT_T(KC_L),  LCTL_T(KC_SPC)
#define ____QWERT__R3___ LT(_FVIM,KC_N),LT(_XVIM,KC_M),LT(_DVIM,KC_ENT),       KC_BSPC,LT(_DALY,KC_ESC)

//#if defined(COMPREHENSIVE_30_LAYOUT)



