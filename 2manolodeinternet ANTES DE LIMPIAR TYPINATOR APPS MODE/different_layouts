 * |        |        |        |        |        |        |        |        |        |        |        |
 * '--------------------------------------------'        '--------------------------------------------'
 */
#define ____IAEOU__L1___         KC_Q,         KC_R,         KC_N,           KC_L,         KC_W
#define ____IAEOU__L2___ LCTL_T(KC_D), LALT_T(KC_S), LSFT_T(KC_T), LGUI_T(KC_SPC),         KC_G
#define ____IAEOU__L3___         KC_Z,         KC_X,         KC_C,           KC_V,         KC_J

#define      ____IAEOU__R1___             KC_Y,         KC_H,         KC_F,         KC_B,          KC_P
#define      ____IAEOU__R2___             KC_I, LGUI_T(KC_A), LSFT_T(KC_E),  LALT_T(KC_O), LCTL_T(KC_U)
#define      ____IAEOU__R3___             KC_K,         KC_M,       KC_ENT,      KC_BSPC,        KC_ESC

