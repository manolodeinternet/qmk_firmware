 * '--------------------------------------------'        '--------------------------------------------'
 *///ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥
//ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥ðŸ”¥

#define ____IAEOU__L1___      TD(APPS_Q),LT(_POWR, KC_R),LT(_LEDS, KC_N),LT(_FUNC, KC_L),LT(_NUMB,KC_W)
#define ____IAEOU__L2___    LCTL_T(KC_D),   LALT_T(KC_S),   LSFT_T(KC_T), LGUI_T(KC_SPC),LT(_ACCN,KC_G)
#define ____IAEOU__L3___ LT(_DALY, KC_Z),LT(_DVIM, KC_X),LT(_MOUS, KC_C),LT(_SYMB, KC_V),LT(_NUMB,KC_J)

#define ____IAEOU__R1___ LT(_SYMB,KC_Y),LT(_FUNC,KC_H),LT(_LEDS, KC_F), LT(_POWR,KC_B),TD(APPS_P)
#define ____IAEOU__R2___ LT(_ACCN,KC_I),LGUI_T(KC_A),  LSFT_T(KC_E),    LALT_T(KC_O),  LCTL_T(KC_U)
#define ____IAEOU__R3___ LT(_FVIM,KC_K),LT(_XVIM,KC_M),LT(_DVIM,KC_ENT),KC_BSPC,       LT(_DALY,KC_ESC)



//#if defined(COMPREHENSIVE_30_LAYOUT)
#elif defined(SIMPLE_30_LAYOUT)
/* [_IAEOU] = LAYER 00 : IAEOU DEFAULT LAYER
 * .--------------------------------------------.        .--------------------------------------------.
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |    Q   |    R   |    N   |    L   |    W   |        |    Y   |    H   |    F   |    B   |    P   |
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |--------+--------+--------+--------+--------|        |--------+--------+--------+--------+--------|
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |    D   |    S   |    T   |SPACEBAR|    G   |        |    I   |    A   |    E   |    O   |    U   |
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |   LCtl |   LAlt |  LSft  |   LGui |        |        |        |  LGui  |  LSft  |   LAlt |   LCtl |
 * |--------+--------+--------+--------+--------|        |--------+--------+--------+--------+--------|
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |    Z   |    X   |    C   |    V   |    J   |        |    K   |    M   |  Enter |Bckspace| Escape |
 * |        |        |        |        |        |        |        |        |        |        |        |
 * |        |        |        |        |        |        |        |        |        |        |        |
 * '--------------------------------------------'        '--------------------------------------------'
 */
#define ____IAEOU__L1___         KC_Q,         KC_R,         KC_N,           KC_L,         KC_W
#define ____IAEOU__L2___ LCTL_T(KC_D), LALT_T(KC_S), LSFT_T(KC_T), LGUI_T(KC_SPC),         KC_G
#define ____IAEOU__L3___         KC_Z,         KC_X,         KC_C,           KC_V,         KC_J

#define      ____IAEOU__R1___             KC_Y,         KC_H,         KC_F,         KC_B,          KC_P
#define      ____IAEOU__R2___             KC_I, LGUI_T(KC_A), LSFT_T(KC_E),  LALT_T(KC_O), LCTL_T(KC_U)
#define      ____IAEOU__R3___             KC_K,         KC_M,       KC_ENT,      KC_BSPC,        KC_ESC

